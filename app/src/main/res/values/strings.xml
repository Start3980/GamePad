    <string name="pref_bluetooth_pairing">Pairing\u2026</string>
    <!-- The connection status of a paired remote bluetooth device. [CHAR_LIMIT=40] -->
    <string name="pref_bluetooth_connecting">Connecting\u2026</string>
    <!-- The connection status of a paired remote bluetooth device. [CHAR_LIMIT=40] -->
    <string name="pref_bluetooth_connected">Connected</string>
    <!-- The connection status of a paired remote bluetooth device. [CHAR_LIMIT=40] -->
    <string name="pref_bluetooth_disconnecting">Disconnecting\u2026</string>
    <!-- The connection status of a paired remote bluetooth device. [CHAR_LIMIT=40] -->
    <string name="pref_bluetooth_disconnected">Disconnected</string>
    <!-- The ability to forget a shared bluetooth device. [CHAR_LIMIT=40] -->
    <string name="pref_bluetooth_forget">Forget</string>

