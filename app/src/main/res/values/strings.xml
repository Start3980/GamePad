<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Name of the app in launcher [CHAR LIMIT=NONE] -->
    <string name="app_name">GamePad</string>

    <!-- Prompt to wait for Bluetooth to turn on [CHAR_LIMIT=16] -->
    <string name="please_wait">Please wait\u2026</string>
    <!-- Generic text to indicate that a setting is disabled. Should be in all caps. [CHAR LIMIT=40] -->
    <string name="generic_disabled">OFF</string>
    <!-- Button label for generic cancel action [CHAR LIMIT=20] -->
    <string name="cancel">Cancel</string>

    <!-- The title of the activity for managing paired bluetooth devices [CHAR LIMIT=40] -->
    <string name="pref_bluetooth">Paired devices</string>
    <!-- The title of the category for unpaired bluetooth devices available to pair with. [CHAR_LIMIT=40] -->
    <string name="pref_bluetoothAvailable">Available devices</string>
    <!-- Title of the list item to be clicked on when the user wants to trigger a new bluetooth device discovery process. [CHAR_LIMIT=60] -->
    <string name="pref_bluetoothScan">Scan for nearby devices</string>
    <!-- Message to be shown during the bluetooth device discovery process. [CHAR_LIMIT=60] -->
    <string name="pref_bluetoothScan_scanning">Scanning for nearby devices\u2026</string>
    <!-- Action to connect to a paired Bluetooth device. [CHAR_LIMIT=60] -->
    <string name="pref_bluetooth_connect">Connect</string>
    <!-- Action to disconnect from a connected paired Bluetooth device. [CHAR_LIMIT=60] -->
    <string name="pref_bluetooth_disconnect">Disconnect</string>
    <!-- Action to unpair a paired Bluetooth device. [CHAR_LIMIT=60] -->
    <string name="pref_bluetooth_unpair">Unpair</string>
    <!-- Action to select a connected paired Bluetooth device. [CHAR_LIMIT=60] -->
    <string name="pref_bluetooth_select">Select</string>
